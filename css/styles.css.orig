
*,
*:before,
*after {
     -moz-box-sizing: border-box; /* Firexfox */
     -webkit-box-sizing: border-box; /* Safari/Chrome/iOS/Android */
     box-sizing: border-box; /* IE */
}
body {
	width: 60%;
	height: 60%;
	margin: 0 auto;
	background-image: url(../images/bgtile.png);
	background-repeat: repeat-x;
}
main {
	font-family: Helvetica, Arial, Sans-Serif;
	width: 95%;
	height: 95%;
	margin: 0 auto;
}
section	{
	width: 45%
	height: 90%;
}
.phones {
	margin-top: 70px;
}
img {
	display: block;
}
#iphones {
	float: left;
	padding-right: 40px;

}
.instagram {
	display: block;
	margin-top: 70px;
	padding-top: 15px;
}
#logo {
	float: left;
}
nav: {
	display: block;
	width: 200px;
	height: 60px
	margin: 0;
}
.test {
	margin-top: 20px;
	margin-right: 50px;
}
a {
	float: left;
	padding: 5px 0 0 50px;
	clear: right;
}
#home {
	display: inline-block;
	position: absolute;
}
}
button {
	clear: both;
}
.button {
 	background: #4d82ac;
 	border-radius: 2px;
 	border: 1px;
 	border-color: #336289;
 	color: white;
 	text-align: right;
 	padding: 0 5px 0 0;
 	display: inline-block;
 	width: 70px;
 	height: 25px;
 	font-size: 12px;
}
